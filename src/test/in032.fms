// these are compound meters!!!
// results should reflect compound meter regardless of time signatures specified
// (except 9/16 ans 21/16 meters, which are "irregular" compound meters 4+1/2 / 8 and 10+1/2 / 8)

clip = (0 20)
comp = yes
timesig = (6 8)
midiin-timescale = 1

part <id flt, 
      inst <name Flute, min-pitch c4, max-pitch c7, 
	    staves <clefs <instclef treble>>, 
	    imports <midiin-track 1>, 
	    export <midiout-prog 73, midiout-artenv yes>>>

part flt
time 0.0
        duration 4 |keysig cmaj|
        voice 1 duration 1.33333 pitch 70.0 dynamic 0.7874 ;
time 1.33333
        voice 1 duration 0.66667 pitch 78.0 dynamic 0.7874 ;
