@macro fomusex{capt, cprfx, prfx, sffx, num}
@float Figure,fig\cprfx\\num\
@multitable @columnfractions 0.3 0.3
@item 
@smallformat
@verbatiminclude \prfx\\num\.\sffx\
@end smallformat
@tab
@image{\prfx\\num\,,,,png}
@end multitable
@caption{\capt\}
@end float
@end macro

@macro ccmoduleex{file, prfx}
@float Figure,mod\file\
@smallformat
@verbatiminclude \file\.\prfx\
@end smallformat
@caption{\file\.\prfx\}
@end float
@end macro

